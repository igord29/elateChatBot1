{
  "name": "elate-moving-chatbot",
  "version": "1.0.0",
  "description": "AI-powered moving company chatbot with real-time support and lead generation",
  "type": "module",
  "main": "backend-api.js",
  "scripts": {
    "start": "node backend-api.js",
    "dev": "nodemon backend-api.js",
    "websocket": "node websocket-server-fixed.js",
    "test": "jest",
    "build": "webpack --mode production",
    "dev:frontend": "webpack serve --mode development",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test:local": "concurrently \"npm run dev\" \"npm run websocket\" \"npm run dev:frontend\"",
    "test:local:backend": "concurrently \"npm run dev\" \"npm run websocket\"",
    "test:local:frontend": "npm run dev:frontend",
    "setup:local": "node scripts/setup-local.js",
    "clean": "rm -rf dist node_modules package-lock.json && npm install"
  },
  "keywords": [
    "chatbot",
    "moving",
    "ai",
    "real-time",
    "lead-generation",
    "websocket"
  ],
  "author": "Elate Moving",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "node-fetch": "^3.3.2",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elate-moving/chatbot.git"
  },
  "bugs": {
    "url": "https://github.com/elate-moving/chatbot/issues"
  },
  "homepage": "https://github.com/elate-moving/chatbot#readme"
}
